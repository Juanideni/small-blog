<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form method="post" action="/compose">
        <div class="form-group">
            <label for="postTitle" >Title</label>
            <input class="form-control" type="text" name="title" id="postTitle"/>
            <label for="postBox">Post</label>
            <textarea class="form-control" name="article" id="postBox" rows="5" cols="30"></textarea>
        </div>
        <button class="btn btn-primary" name="submitButton" type="submit">Publish</button>
    </form>
<%- include("partials/footer"); -%>